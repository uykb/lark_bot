# 飞书应用凭证
APP_ID=your_app_id
APP_SECRET=your_app_secret

# Webhook配置（可选）
WEBHOOK_URL=your_webhook_url

# 消息接收者配置
CHAT_ID=default_chat_id                  # 默认群聊ID
ADDITIONAL_CHAT_IDS=chat_id1,chat_id2    # 额外的群聊ID，用逗号分隔
DEFAULT_USER_ID=default_user_id          # 默认用户ID

# 消息展示配置
USE_CARD=true                           # 是否使用卡片消息（true/false）